istLoading = false\r\n      })\r\n    },\r\n    handleFilter() {\r\n      this.listQuery.page_no = 1\r\n      this.getList()\r\n    },\r\n    handleDelete(row, index) {\r\n      this.$confirm('此操作将删除该记录, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        const deleteQuery = {\r\n          'id': row.id\r\n        }\r\n        serviceDelete(deleteQuery).then(response => {\r\n          this.$notify({\r\n            title: 'Success',\r\n            message: '删除成功',\r\n            type: 'success',\r\n            duration: 2000\r\n          })\r\n          this.getList()\r\n        })\r\n      }).catch(() => {\r\n        this.$notify({\r\n          title: 'Success',\r\n          message: '已取消删除',\r\n          type: 'info',\r\n          duration: 2000\r\n        })\r\n      })\r\n\r\n      // this.list.splice(index, 1)\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}