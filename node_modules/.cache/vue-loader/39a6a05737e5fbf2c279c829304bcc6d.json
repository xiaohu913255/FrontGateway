omponents-container\">\r\n    <el-row>\r\n      <el-form ref=\"form\" :model=\"form\" label-width=\"130px\">\r\n        <el-card class=\"box-card\" style=\"margin-bottom:20px;\">\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span v-if=\"is_edit === false\">创建租户</span>\r\n            <span v-if=\"is_edit === true\">修改租户</span>\r\n          </div>\r\n          <div>\r\n            <el-form-item label=\"app_id\" class=\"is-required\">\r\n              <el-input v-model=\"form.app_id\" :disabled=\"is_edit===true\" placeholder=\"6-128位字母数字下划线\" />\r\n            </el-form-item>\r\n            <el-form-item label=\"租户名称\" class=\"is-required\">\r\n              <el-input v-model=\"form.name\" placeholder=\"最多255字符，必填\" />\r\n            </el-form-item>\r\n            <el-form-item label=\"32位密钥\">\r\n              <el-input v-model=\"form.secret\" placeholder=\"32位字符串，非必填，自动生成\" />\r\n            </el-form-item>\r\n\r\n            <el-form-item label=\"QPS限流\"><el-input v-model=\"form.qps\" placeholder=\"0表示无限制\" />\r\n            </el-form-item>\r\n\r\n            <el-form-item label=\"日请求量限流\">\r\n              <el-input v-model=\"form.qpd\" placeholder=\"0表示无限制\" />\r\n            </el-form-item>\r\n\r\n            <el-form-item>\r\n              <el-button type=\"primary\" :loading=\"disableButton\" @click=\"onSubmit\">立即提交</el-button>\r\n            </el-form-item>\r\n          </div>\r\n        </el-card>\r\n      </el-form>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  appDetail,\r\n  appAdd,\r\n  appUpdate\r\n} from '@/api/app'\r\nexport default {\r\n  components: { },\r\n  data() {\r\n    return {\r\n      is_edit: false,\r\n      disableButton: false,\r\n      form: {\r\n        id: '0',\r\n        app_id: '',\r\n        name: '',\r\n        secret: '',\r\n        white_ips: '',\r\n        qpd: '',\r\n        qps: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    const id = this.$route.params && this.$route.params.id\r\n    if (id > 0) {\r\n      this.is_edit = true\r\n      this.fetchData(id)\r\n    }\r\n    this.tempRoute = Object.assign({}, this.$route)\r\n  },\r\n  methods: {\r\n    fetchData(id) {\r\n      this.listLoading = true\r\n      const query = {\r\n        id: id\r\n      }\r\n      appDetail(query).then(response => {\r\n        const formData = response.data\r\n        this.form = formData\r\n        this.listLoading = false\r\n      })\r\n    },\r\n    onSubmit() {\r\n      this.disableButton = true\r\n      const formData = Object.assign({}, this.form)\r\n      formData.id = Number(formData.id)\r\n      formData.qpd = Number(formData.qpd)\r\n      formData.qps = Number(formData.qps)\r\n\r\n      if (this.is_edit) {\r\n        appUpdate(formData).then(() => {\r\n          this.disableButton = false\r\n          this.$notify({\r\n            title: 'Success',\r\n            message: 'Submit Successfully',\r\n            type: 'success',\r\n            duration: 500\r\n          })\r\n        }, () => {\r\n          this.disableButton = false\r\n        })\r\n      } else {\r\n        appAdd(formData).then(() => {\r\n          this.disableButton = false\r\n          this.$notify({\r\n            title: 'Success',\r\n            message: 'Submit Successfully',\r\n            type: 'success',\r\n            duration: 500\r\n          })\r\n        }, () => {\r\n          this.disableButton = false\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.mixin-components-container {\r\n  background-color: #f0f2f5;\r\n  padding: 30px;\r\n  min-height: calc(100vh - 84px);\r\n}\r\n.component-item{\r\n  min-height: 100px;\r\n}\r\n.el-select .el-input {\r\n    width: 130px;\r\n  }\r\n.input-with-select .el-input-group__prepend {\r\n  background-color: #fff;\r\n}\r\n</style>\r\n"]}]}